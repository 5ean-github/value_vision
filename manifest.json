{
    "manifest_version": 3,
    "name": "H&M Clothes Price Extractor",
    "version": "1.0",
  
    "description": "Extracts prices from the H&M website.",

  
    "permissions": [
      "activeTab"
    ],
  
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }
  